{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug LCH-Farrow1 (Linux)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/LCH-Farrow1",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                },
                {
                    "description": "Show source code in disassembly",
                    "text": "-gdb-set disassemble-next-line on",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build (Linux)",
            "miDebuggerPath": "/usr/bin/gdb",
            "logging": {
                "moduleLoad": false,
                "trace": false,
                "engineLogging": false,
                "programOutput": true,
                "exceptions": true
            },
            "justMyCode": false,
            "showDisplayString": true
        },
        {
            "name": "Debug LCH-Farrow1 (Linux, Attach to Process)",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/build/LCH-Farrow1",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}
